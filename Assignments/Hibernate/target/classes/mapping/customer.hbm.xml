<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.model.Customer" table="CUSTOMER">
		<id name="id" column="C_ID">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="org.hibernate.type.StringType" column="C_NAME"></property>
		<property name="tel" type="org.hibernate.type.StringType" column="C_TEL"></property>
	
	</class>
	
</hibernate-mapping>