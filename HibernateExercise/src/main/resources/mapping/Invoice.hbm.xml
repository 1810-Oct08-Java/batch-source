<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.revature.model.Invoice" table="INVOICE">
		<id name="invoiceId" column="INVOICE_ID">
			<generator class="increment"></generator>
		</id>
		<property name="date" column="INVOICE_DATE" type="org.hibernate.type.DateType"></property>
		<property name="amount" column="INVOICE_AMOUNT" type="org.hibernate.type.DoubleType"></property>
	</class>
</hibernate-mapping>