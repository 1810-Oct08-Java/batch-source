<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "./Hibernate/hibernate Mapping DTD 3.0/EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.model.Customer" table="CUSTOMER">
		<id name="id" column="CUSTOMER_ID">
			<generator class="increment"></generator>
		</id>
		<property name="name" type="org.hibernate.type.StringType" column="CUSTOMER_NAME"></property>
		<property name="phoneNumber" type="org.hibernate.type.StringType" column="PHONE_NUMBER"></property>
	</class>
</hibernate-mapping>