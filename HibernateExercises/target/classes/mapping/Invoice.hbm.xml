<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "./Hibernate/hibernate Mapping DTD 3.0/EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.revature.model.Invoice" table="INVOICE">
		<id name="id" column="INVOICE_ID">
			<generator class="increment"></generator>
		</id>
		<property name="invoiceDate" type="org.hibernate.type.DateType"></property>
		<property name="amount" type="org.hibernate.type.DoubleType"></property>
	</class>
</hibernate-mapping>